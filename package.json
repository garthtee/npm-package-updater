{
  "author": "Garth Toland",
  "name": "npm-package-updater",
  "displayName": "NPM Package Updater",
  "description": "Update your dependencies to the latest automatically, straight from NPM.",
  "version": "1.1.0",
  "publisher": "GarthToland",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:npm-package-updater.updateLatestVersions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/garthtee/npm-package-updater.git"
  },
  "bugs": {
    "url": "https://github.com/garthtee/npm-package-updater/issues"
  },
  "homepage": "https://github.com/garthtee/npm-package-updater#readme",
  "license": "MIT",
  "main": "./out/extension.js",
  "icon": "./src/images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "npm-package-updater.updateLatestVersions",
        "title": "Update All Latest: NPM Package Updater"
      }
    ],
    "keybindings": [
      {
        "command": "npm-package-updater.updateLatestVersions",
        "key": "ctrl+shift+u",
        "mac": "ctrl+shift+u"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "tidy": "npx prettier --write .",
    "tidy:check": "npx prettier --check .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.20.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "prettier": "2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "husky": "^6.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}

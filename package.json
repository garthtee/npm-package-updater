{
	"author": "Garth Toland",
	"name": "npm-package-updater",
	"displayName": "npm-package-updater",
	"description": "Update your dependencies at to the latest straight from NPM.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:npm-package-updater.updateLatestVersions"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/garthtee/npm-package-updater.git"
	},
	"bugs": {
		"url": "https://github.com/garthtee/npm-package-updater/issues"
	},
	"homepage": "https://github.com/garthtee/npm-package-updater#readme",
	"license": "MIT",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "npm-package-updater.updateLatestVersions",
				"title": "Update All Latest: NPM Package Updater"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
    "tidy": "npx prettier --write ."
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.20.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"prettier": "2.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^0.21.1"
	}
}
